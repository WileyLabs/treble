{
  "name": "treble",
  "version": "0.0.1",
  "description": "Treble extracts triples from the Web while you browse.",
  "repository": "WileyLabs/treble",
  "main": "null",
  "scripts": {
    "build": "npm run build:js-collect && npm run build:js-download",
    "build:js-collect": "browserify -s collect src/collect.js -t babelify --outfile extension/lib/collect.js",
    "build:js-download": "browserify -s download src/download.js -t babelify --outfile extension/lib/download.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "RDFa",
    "JSON-LD",
    "Linked",
    "Data",
    "WebExtension"
  ],
  "author": "John Wiley & Sons, Inc.",
  "contributors": [
    {
      "name": "BigBlueHat",
      "email": "byoung@bigbluehat.com"
    }
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babelify": "^7.3.0"
  },
  "dependencies": {
    "jsonld": "^0.4.11",
    "jsonld-rdfa-parser": "^1.5.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
